{
  "name": "spryker/merchant-product-data-import",
  "description": "MerchantProductDataImport module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/kernel": "^3.33.0",
    "spryker/data-import": "^1.10.0",
    "spryker/merchant": "^2.0.0",
    "spryker/merchant-product": "^0.1.0",
    "spryker/product": "^6.0.0"
  },
  "require-dev": {
    "spryker/testify": "*",
    "spryker/propel": "*",
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Shared\\MerchantProductDataImport\\Helper\\": "tests/SprykerTest/Shared/MerchantProductDataImport/Helper/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
